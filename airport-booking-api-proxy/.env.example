# Environment Variables Template
# Copy this file to .env and fill in your actual values

# Server Configuration
PORT=3001
NODE_ENV=development

# Google Maps API Key (server-side, unrestricted)
# This should be a server-side API key with IP restrictions
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# CORS Configuration
# Comma-separated list of allowed origins
ALLOWED_ORIGINS=http://localhost:3000,https://yourdomain.com

# Rate Limiting Configuration
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Optional: Database Configuration (for future use)
# DATABASE_URL=your_database_connection_string

# Optional: Redis Configuration (for session storage)
# REDIS_URL=redis://localhost:6379

# Optional: JWT Secret (for authentication)
# JWT_SECRET=your_jwt_secret_here

# Optional: Webhook URLs for notifications
# SLACK_WEBHOOK_URL=your_slack_webhook_url
# DISCORD_WEBHOOK_URL=your_discord_webhook_url
